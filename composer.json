{
    "name": "rakasch/cakephp-cors",
    "description": "A CakePHP (5.x) plugin for activate cors domain in your application",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=8.0.0",
        "cakephp/cakephp": "^5.0",
        "psr/http-server-handler": "^1.0",
        "psr/http-server-middleware": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.0",
        "cakephp/cakephp-codesniffer": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Cors\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cors\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
